import java.io.*;
import java.util.Scanner;
class Circle{
    double cf;
    double circumference(){
        return (22/7.0)*cf*2;
    }
}

public class Main{
    public static void main(String[] args){
        Circle c1 = new Circle();
        Circle c2 = new Circle();
        c1.cf = 5.6;
        System.out.println("circumference of the circle1:"+c1.circumference());
        c2.cf = 4.7;
        System.out.println("circumference of the circle1:"+c2.circumference());
    }
}
